<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.trivia_project.myquizproject">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.trivia_project.myquizproject.gameOver" />
        <activity android:name="com.example.trivia_project.myquizproject.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.trivia_project.myquizproject.results" />
        <activity android:name="com.example.trivia_project.myquizproject.gamePage" />
        <activity android:name="com.example.trivia_project.myquizproject.HighScore" />
        <activity android:name="com.example.trivia_project.myquizproject.server_Connection"></activity>
    </application>

</manifest>